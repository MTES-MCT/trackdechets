{
  "name": "td-doc",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "prebuild": "yarn build-workflow-plugin && yarn docs",
    "predev": "yarn build-workflow-plugin && yarn docs",
    "dev": "docusaurus start",
    "start": "npx serve -s build -l $PORT",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "docs": "yarn docs:user-company && yarn docs:bsdd && yarn docs:bsdasri && yarn docs:bsff && yarn docs:bsda && yarn docs:bsvhu && yarn docs:registry && yarn docs:webhooks",
    "docs:user-company": "docusaurus docs:generate:graphql:user-company",
    "docs:bsdd": "docusaurus docs:generate:graphql:bsdd",
    "docs:bsdasri": "docusaurus docs:generate:graphql:bsdasri",
    "docs:bsff": "docusaurus docs:generate:graphql:bsff",
    "docs:bsda": "docusaurus docs:generate:graphql:bsda",
    "docs:bsvhu": "docusaurus docs:generate:graphql:bsvhu",
    "docs:registry": "docusaurus docs:generate:graphql:registry",
    "docs:webhooks": "docusaurus docs:generate:graphql:webhooks",
    "build-workflow-plugin": "tsc --project plugin/tsconfig.json && copyfiles -u 1 ../back/src/**/*.gql plugin/build"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.0",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^6.0.0",
    "clsx": "^1.1.1",
    "docusaurus-graphql-plugin": "^0.8.0",
    "file-loader": "^6.2.0",
    "graphql": "^16.5.0",
    "mermaid": "^9.4.2",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.0",
    "@docusaurus/types": "^2.4.0",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.13",
    "copyfiles": "^2.4.1",
    "typescript": "^4.3.5"
  }
}
