{
  "extends": ["../.eslintrc.json"],
  "ignorePatterns": [
    "!**/*",
    "prisma/seed*.ts",
    "src/common/plugins/graphiql/**",
    "graphql-markdown-plugin.js",
    "jest.config.integration.ts"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "parserOptions": {
        "project": ["back/tsconfig.*?.json"]
      },
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
