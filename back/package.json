{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "npm run generate",
    "dev": "npm run watch:all",
    "watch:all": "concurrently -k -p \"[{name}]\" -n \"Api,Queue,Notifier\" -c \"yellow.bold,cyan.bold,blue.bold\" \"npm run watch:node\" \"npm run watch:queue\" \"npm run watch:notifier\"",
    "watch:node": "nodemon -r dotenv/config --watch src --exec ts-eager --inspect=0.0.0.0:9229 src/index.ts -e ts,tsx",
    "start": "node dist/src/index.js",
    "update": "node dist/prisma/scripts/index.js",
    "update:dev": "ts-node -r dotenv/config prisma/scripts/index.ts",
    "migrate": "node dist/prisma/migrate.js",
    "migrate:dev": "ts-node -r dotenv/config prisma/migrate.ts",
    "check:migrations": "pg-validate-migrations ./prisma/migrations",
    "test": "jest",
    "preintegration-tests": "prisma db push && npm run generate:graphql && npm run reindex-all-bsds-bulk:dev",
    "integration-tests": "jest --config ./integration-tests/jest.config.js -i --forceExit",
    "types": "tsc --noEmit",
    "generate": "npm run generate:prisma && npm run generate:graphql",
    "generate:prisma": "prisma generate",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "prebuild": "npm run generate",
    "build": "tsc && copyfiles -e **/*.ts src/**/*.{graphql,pdf,png,ttf,html,css,svg,wsdl} src/**/assets/*.js prisma/**/*.{prisma,sql} dist/",
    "scalingo-postbuild": "./src/scripts/build/scalingo.sh",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "lint:fix": "eslint -c .eslintrc.js --fix --ext .ts ./src",
    "bulk-create-account": "node dist/src/users/bulk-creation/index.js",
    "bulk-create-account:dev": "ts-node src/users/bulk-creation/index.ts",
    "reindex-partial-in-place": "node dist/src/scripts/bin/reindexPartialInPlace.js",
    "reindex-partial-in-place:dev": "ts-node -r dotenv/config ./src/scripts/bin/reindexPartialInPlace.ts",
    "reindex-all-bsds-bulk": "LOG_TO_HTTP=1 node dist/src/scripts/bin/reindexAllInBulk.js",
    "reindex-all-bsds-bulk:dev": "ts-node  -r dotenv/config ./src/scripts/bin/reindexAllInBulk.ts -- -d",
    "reindex-bsd": "node dist/src/scripts/bin/reindexBsd.js $bsdId",
    "reindex-bsd:dev": "ts-node -r dotenv/config ./src/scripts/bin/reindexBsd.ts $bsdId",
    "watch:apiqueue": "nodemon -r dotenv/config --watch src --exec \"ts-eager\" --inspect=0.0.0.0:9230 src/queue/consumer.ts -e ts",
    "watch:indexationqueue": "nodemon -r dotenv/config --watch src --exec \"ts-eager\" --inspect=0.0.0.0:9231 src/queue/consumerIndexation.ts -e ts",
    "watch:queue": "concurrently -k -p \"[{name}]\" -n \"Api Queue,Indexation Queue\" -c \"yellow.bold,cyan.bold\" \"npm run watch:apiqueue\" \"npm run watch:indexationqueue\"",
    "watch:notifier": "nodemon -r dotenv/config --watch src --exec \"ts-eager\" src/notifier/index.ts -e ts",
    "queue": "node dist/src/queue/consumer.js",
    "queue:obliterate": "node dist/src/queue/obliterate.js",
    "purge-pdf-token": "node dist/src/scripts/bin/purgePdfAccessToken.js",
    "purge-pdf-token:dev": "ts-node -r dotenv/config ./src/scripts/bin/purgePdfAccessToken.ts"
  },
  "engines": {
    "node": "^16"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
