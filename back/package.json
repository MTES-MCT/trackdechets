{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "copyfiles src/common/constants/** ../front/src/generated/constants --flat && npx prisma generate",
    "dev": "concurrently npm:watch-node npm:watch-graphql",
    "watch-node": "npx nodemon -r dotenv/config --watch src --exec \"ts-eager\" src/index.ts",
    "watch-graphql": "npm run graphql-codegen -- --watch",
    "start": "node dist/src/index.js",
    "update": "node prisma/scripts/index.js",
    "update:dev": "ts-node prisma/scripts/index.ts",
    "migrate": "node dist/prisma/migrate.js",
    "migrate:dev": "ts-node -r dotenv/config prisma/migrate.ts",
    "check:migrations": "pg-validate-migrations ./prisma/migrations",
    "test": "jest",
    "types": "tsc --noEmit",
    "generate": "npx prisma generate",
    "prebuild": "npm run generate",
    "build": "tsc && copyfiles -e **/*.ts src/**/*.{graphql,pdf,png,ttf,html,css} prisma/**/*.{prisma,sql} dist/ && npm run download-scalingo-cert",
    "download-scalingo-cert": "node dist/src/scripts/bin/downloadScalingoCert.js",
    "download-scalingo-cert:dev": "ts-node -r dotenv/config ./src/scripts/bin/downloadScalingoCert.ts",
    "scalingo-postbuild": "npm run build && cd dist && npm run migrate && npm run index-elastic-search",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "lint:fix": "eslint -c .eslintrc.js --fix --ext .ts ./src",
    "bulk-create-account:dev": "ts-node src/users/bulk-creation/index.ts",
    "bulk-create-account": "node src/users/bulk-creation/index.js",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "index-elastic-search": "node dist/src/scripts/bin/indexElasticSearch.js",
    "index-elastic-search:dev": "ts-node -r dotenv/config ./src/scripts/bin/indexElasticSearch.ts"
  },
  "engines": {
    "node": "^14"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
