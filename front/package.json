{
  "name": "bnsd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate": "npm run generate:constants && npm run generate:graphql && npm run generate:tailwind",
    "generate:dev": "npm run generate:constants && npm run generate:graphql && npm run generate:tailwind:dev",
    "generate:constants": "copyfiles ../back/src/common/constants/** ./src/generated/constants --flat",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:tailwind": "NODE_ENV=production tailwindcss build src/tailwind.css -o src/generated/css/tailwind.generated.css",
    "generate:tailwind:dev": "tailwindcss build src/tailwind.css -o src/generated/css/tailwind.generated.css",
    "predev": "npm run generate:dev",
    "dev": "vite --host",
    "prebuild": "npm run generate && tsc",
    "build": "bash bin/build.sh",
    "build-prod": "NODE_ENV=production vite build",
    "test": "jest src",
    "start": "node server.js",
    "types": "tsc --noEmit",
    "supportedBrowsers": "echo \"// eslint-disable-next-line\nmodule.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > src/supportedBrowsers.js",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "run-storybook": "npx http-server storybook-static/"
  },
  "engines": {
    "node": "^18"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:import/typescript"
    ],
    "plugins": [
      "no-only-tests",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-only-tests/no-only-tests": "error"
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@dataesr/react-dsfr": "^3.5.6",
    "@reach/tooltip": "^0.11.2",
    "@sentry/browser": "^6.19.7",
    "body-parser": "^1.20.1",
    "classnames": "^2.2.6",
    "cleave.js": "^1.6.0",
    "cogo-toast": "^4.1.3",
    "constant-case": "^3.0.3",
    "copy-text-to-clipboard": "^3.0.1",
    "date-fns": "^2.28.0",
    "decimal.js-light": "^2.5.1",
    "express": "^4.18.2",
    "focus-trap-react": "^10.0.2",
    "formik": "2.2.5",
    "graphql": "^16.6.0",
    "graphql-anywhere": "^4.2.7",
    "helmet": "^6.0.1",
    "jsvat": "^2.5.3",
    "libphonenumber-js": "^1.10.8",
    "local-storage": "^2.0.0",
    "mapbox-gl": "^1.11.1",
    "node-fetch": "^2.6.7",
    "omit-deep-lodash": "^1.1.6",
    "plausible-tracker": "^0.3.8",
    "query-string": "^6.10.1",
    "rc-tree": "^5.7.3",
    "react": "18.2.0",
    "react-app-polyfill": "^1.0.6",
    "react-aria": "^3.24.0",
    "react-datepicker": "^3.4.1",
    "react-dom": "18.2.0",
    "react-multi-select-component": "^4.3.4",
    "react-qr-code": "^1.0.5",
    "react-router-dom": "5.3.4",
    "react-select": "^3.1.0",
    "react-stately": "^3.22.0",
    "react-switch": "5.0.1",
    "react-table": "^7.8.0",
    "react-tabs": "3.1.1",
    "remixicon": "^2.5.0",
    "tailwindcss": "^1.9.6",
    "text-security": "^3.2.1",
    "use-media": "^1.4.0",
    "uuid": "^9.0.0",
    "world-countries": "^4.0.0",
    "yup": "^0.32.11",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@storybook/addon-a11y": "^6.5.14",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
