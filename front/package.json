{
  "name": "bnsd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@reach/dialog": "^0.11.2",
    "@reach/menu-button": "^0.11.2",
    "@reach/tooltip": "^0.11.2",
    "@sentry/browser": "^5.28.0",
    "@sentry/react": "^5.28.0",
    "@types/mapbox-gl": "^0.54.5",
    "@types/react-table": "^7.0.29",
    "classnames": "^2.2.6",
    "cleave.js": "^1.6.0",
    "cogo-toast": "^4.1.3",
    "constant-case": "^3.0.3",
    "date-fns": "^2.16.1",
    "env-cmd": "^10.0.1",
    "formik": "2.2.5",
    "graphql": "^14.0.2",
    "graphql-anywhere": "^4.2.7",
    "jwt-decode": "^2.2.0",
    "local-storage": "^2.0.0",
    "mapbox-gl": "^1.11.1",
    "node-sass": "^4.14.1",
    "object.omit": "^3.0.0",
    "query-string": "^6.10.1",
    "rc-tree": "^1.14.10",
    "react": "16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-datepicker": "^3.4.1",
    "react-dom": "16.13.1",
    "react-lazyload": "^2.5.0",
    "react-qr-code": "^1.0.5",
    "react-router-dom": "5.1.2",
    "react-router-tabs": "^1.3.2",
    "react-scripts": "3.4.3",
    "react-select": "^3.1.0",
    "react-switch": "5.0.1",
    "react-table": "^7.6.3",
    "react-tabs": "3.1.1",
    "tailwindcss": "^1.4.6",
    "use-media": "^1.4.0",
    "uuid": "^8.3.1",
    "world-countries": "^4.0.0",
    "yup": "^0.32.9",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "generate": "npm run generate:constants && npm run generate:graphql && npm run generate:tailwind",
    "generate:constants": "copyfiles ../back/src/common/constants/** ./src/generated/constants --flat",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:tailwind": "tailwindcss build src/tailwind.css -o src/generated/css/tailwind.generated.css",
    "predev": "npm run generate",
    "dev": "react-scripts start",
    "prebuild": "npm run generate",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "start": "npx serve -s build -l $PORT",
    "types": "tsc --noEmit",
    "eject": "react-scripts eject",
    "supportedBrowsers": "echo \"// eslint-disable-next-line\nmodule.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > src/supportedBrowsers.js",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src"
  },
  "engines": {
    "node": "^14"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:import/recommended",
      "plugin:import/typescript"
    ],
    "plugins": [
      "no-only-tests",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-only-tests/no-only-tests": "error"
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-resolvers": "^1.19.2",
    "@testing-library/dom": "^6.6.0",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.3",
    "@types/classnames": "^2.2.10",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.9.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^10.17.27",
    "@types/node-fetch": "^2.1.7",
    "@types/rc-tree": "^1.11.3",
    "@types/react": "^16.9.43",
    "@types/react-datepicker": "^3.1.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-lazyload": "^2.5.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.16",
    "@types/react-tabs": "^2.3.2",
    "@types/uuid": "^8.3.0",
    "@types/zxcvbn": "^4.4.0",
    "@typescript-eslint/parser": "^3.0.2",
    "browserslist-useragent-regexp": "^2.0.5",
    "copyfiles": "^2.4.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "prettier": "^2.0.5",
    "typescript": "^4.2.4"
  }
}
