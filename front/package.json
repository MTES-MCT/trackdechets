    "@storybook/testing-library": "0.0.13",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/classnames": "^2.2.10",
    "@types/cleave.js": "^1.4.6",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^29.5.0",
    "@types/mapbox-gl": "^0.54.5",
    "@types/node": "^18.15.5",
    "@types/node-fetch": "^2.1.7",
    "@types/react": "^18.0.28",
    "@types/react-datepicker": "^3.1.2",
    "@types/react-dom": "^18.0.11",
    "@types/react-lazyload": "^2.5.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^3.0.16",
    "@types/react-table": "^7.7.14",
    "@types/react-tabs": "^2.3.2",
    "@types/zxcvbn": "^4.4.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.3.0",
    "browserslist-useragent-regexp": "^2.0.5",
    "copyfiles": "^2.4.1",
    "core-js": "^3.21.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.21.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^2.8.6",
    "sass": "^1.49.7",
    "storybook-addon-apollo-client": "^4.1.4",
    "storybook-addon-designs": "^6.3.1",
    "typescript": "^5.0.2",
    "vite": "^3.2.5",
    "vite-plugin-svgr": "^1.0.1"
  }
}
