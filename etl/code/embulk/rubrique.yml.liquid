in:
  type: file
  path_prefix: {{ env.DATA_DIR }}/s3ic/rubriques/rubriques.csv
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: ;
    quote: '"'
    escape: '"'
    trim_if_not_quoted: false
    skip_header_lines: 1
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: code_s3ic, type: string}
    - {name: rubrique, type: string}
    - {name: alinea, type: string}
    - {name: date_autorisation, type: string}
    - {name: etat_activite, type: string}
    - {name: regime_autorise, type: string}
    - {name: activite, type: string}
    - {name: volume, type: string}
    - {name: unite, type: string}
out:
  type: postgresql
  host: {{ env.POSTGRES_HOST }}
  user: {{ env.POSTGRES_USER }}
  password: {{ env.POSTGRES_PASSWORD }}
  database: {{ env.POSTGRES_DATABASE }}
  port: {{ env.POSTGRES_PORT }}
  table: rubrique_source
  schema: etl
  options: {loglevel: 2}
  mode: replace
